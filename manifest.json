{
  "name": "Custom Emojis for Messenger",
  "version": "1.0",
  "description": "Make custom emojis and use them on facebook messenger!",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
    },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*", "https://www.facebook.com/", "https://www.messenger.com/", "https://www.messenger.com/*"
      ],
      "js": ["jquery.js","content.js"]
    }
  ]
}
